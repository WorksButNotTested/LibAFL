{
	"name": "LibAFL Offline",
	"image": "libafl-offline",
	"customizations": {
		"vscode": {
			"extensions": [
				"rust-lang.rust-analyzer",
			],
			"settings": {
				"rust-analyzer.cargo.noDefaultFeatures": true
			}
		}
	},
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined",
	],
	"mounts": [
    "source=${localWorkspaceFolder},target=/home/ws,type=bind",
    "source=${localEnv:HOME},target=/home/share,type=bind"
  ],
}
